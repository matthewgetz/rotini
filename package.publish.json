{
  "name": "rotini",
  "description": "a framework for building node.js cli programs",
  "version": "1.2.0",
  "license": "MIT",
  "author": "Matt Getz",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "https://github.com/matthewgetz/rotini.git",
  "bugs": "https://github.com/matthewgetz/rotini/issues",
  "homepage": "https://matthewgetz.github.io/rotini/",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "rotini": "./build/index.global.js"
  },
  "exports": {
    ".": {
      "import": "./build/index.mjs",
      "require": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "build"
  ],
  "keywords": [
    "rotini",
    "cli",
    "framework",
    "commands",
    "arguments",
    "flags",
    "argv"
  ],
  "publishConfig": {
    "access": "public"
  }
}
